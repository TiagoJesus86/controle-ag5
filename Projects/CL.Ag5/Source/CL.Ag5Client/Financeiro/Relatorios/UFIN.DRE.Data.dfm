      '  AND ( VND.ATIVO = '#39'T'#39' )'
      '  AND ( VND.DATA_VENDA BETWEEN |DI AND |DF )'
      '  AND ( VNDI.KVND_PEDIDO_ITEM IS NOT NULL )'
      '  AND ( VND.VALOR_DEDUCOES > 0 )'
      '  AND (        (  |TIPOCC = 0 )'
      
        '            OR ( (  |TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  |TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
