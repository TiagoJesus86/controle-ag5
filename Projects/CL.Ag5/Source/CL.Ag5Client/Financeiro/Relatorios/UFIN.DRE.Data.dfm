      '*/'
      'UNION ALL'
      'SELECT'
      '  A.KFIN_PLANOCONTA'
      ', CAST( NULL AS SYS$FKGUID20 ) KEST_GRUPOPRODUTO'
      ', '#39'C'#39' DEBITO_CREDITO'
      ', COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) DATA'
      ', (COALESCE( AP.PORCENTAGEM / 100.00, 1.00 ) * PA.VALOR ) VALOR'
      'FROM FIN_ARECEBER A'
      
        'LEFT JOIN FIN_ARECEBER_PARCELAS PA ON PA.KFIN_ARECEBER = A.KFIN_' +
        'ARECEBER'
      
        '-- LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  = A.KFIN' +
        '_PLANOCONTA'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_ARECEBER'#39' AND AP.TABLEKEY  = A.KFIN_ARECEBER'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '    AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( A.TABLENAME IS NULL )'
      '  AND ( :PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL ) '
      
        '  AND ( COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) BETWEEN' +
        ' :DI AND :DF ) '
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
