      'UNION ALL'
      'SELECT'
      '  DVPI.KFIN_PLANOCONTA'
      ', CAST( NULL AS SYS$FKGUID20 ) KEST_GRUPOPRODUTO'
      ', '#39'C'#39' DEBITO_CREDITO'
      ', COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) DATA'
      
        '--, IIF( (COALESCE( AP.PORCENTAGEM / 100.00, 1.00 ) * DVPI.TOTAL' +
        '  ) > 0,'
      
        '--     - (COALESCE( AP.PORCENTAGEM / 100.00, 1.00 ) * DVPI.TOTAL' +
        '  ),'
      
        '--       (COALESCE( AP.PORCENTAGEM / 100.00, 1.00 ) * DVPI.TOTAL' +
        '  )) VALOR'
      ''
      
        ', IIF(  (COALESCE( AP.PORCENTAGEM / 100.00, 1.00 ) * (PA.VALOR *' +
        ' (DVPI.TOTAL / COALESCE(NULLIF(DVP.TOTAL_NOTA,0),1) ))) > 0'
      
        '   ,  - (COALESCE( AP.PORCENTAGEM / 100.00, 1.00 ) * (PA.VALOR *' +
        ' (DVPI.TOTAL / COALESCE(NULLIF(DVP.TOTAL_NOTA,0),1) )))'
      
        '   ,    (COALESCE( AP.PORCENTAGEM / 100.00, 1.00 ) * (PA.VALOR *' +
        ' (DVPI.TOTAL / COALESCE(NULLIF(DVP.TOTAL_NOTA,0),1) ))) ) VALOR'
      'FROM DEV_PEDIDOS DVP'
      
        'LEFT JOIN DEV_PEDIDO_ITENS DVPI ON DVPI.KDEV_PEDIDO = DVP.KDEV_P' +
