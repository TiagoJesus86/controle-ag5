      'INNER JOIN FIN_APAGAR A ON A.KFIN_APAGAR = PA.KFIN_APAGAR'
      
        '-- LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  =  pPLAN' +
        'OCONTA_FINANCIAMENTO'
      'WHERE'
      '      (  (  :TIPOCC = 0 )   OR  (  :TIPOCC = 1 ) )'
      '  AND ( A.TABLENAME = '#39'FIN_FINANCIAMENTOS_GERAL'#39' )'
      '  AND ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( FP.AMORTIZACAO > 0 )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( :PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL )'
      
        '  AND ( COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) BETWEEN' +
        ' :DI AND :DF )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
