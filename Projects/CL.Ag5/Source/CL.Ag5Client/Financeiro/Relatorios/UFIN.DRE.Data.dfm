      'FROM FIN_APAGAR_PARCELAS PA'
      'INNER JOIN FIN_APAGAR A ON A.KFIN_APAGAR = PA.KFIN_APAGAR'
      
        '-- LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  = PA.KFI' +
