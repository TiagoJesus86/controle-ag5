      'UNION ALL'
      'SELECT'
      '  PA.KFIN_PLANOCONTA_JURO_DESCONTO'
      ', CAST( NULL AS SYS$FKGUID20 ) KEST_GRUPOPRODUTO'
      ', '#39'C'#39' DEBITO_CREDITO'
      ', COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) DATA'
      
        ', (COALESCE(  AP.PORCENTAGEM / 100.00, 1.00 ) * ABS( PA.JURO_DES' +
