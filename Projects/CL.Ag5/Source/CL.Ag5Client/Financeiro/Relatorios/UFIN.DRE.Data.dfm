    SQLConnection = DBFinanceiro.SQLDB
    SQL = (
      'SELECT -- D.R.E. Competencia'
      '    KFIN_PLANOCONTA'
      '  , DATA - EXTRACT( DAY FROM DATA  ) + 1 MES'
      '  , DEBITO_CREDITO'
      '  , SUM( VALOR ) VALOR'
      'FROM ('
      'SELECT'
      '   A.KFIN_PLANOCONTA'
      ' , '#39'D'#39' DEBITO_CREDITO'
      ' , IIF ( A.RECORRENCIA = '#39'F'#39', A.DATALANCTO, PA.VENCTO ) DATA'
      '-- , A.HISTORICO'
      
        ', (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * IIF ( A.RECORRENCI' +
        'A = '#39'F'#39', A.VALOR, PA.VALOR  ) ) VALOR'
      'FROM FIN_APAGAR A'
      
        'LEFT JOIN ( SELECT P.KFIN_APAGAR, P.VENCTO, P.PRORROGACAO, P.PAG' +
        'TO, P.VALOR'
      '              FROM FIN_APAGAR_PARCELAS P'
      '              JOIN FIN_APAGAR C ON C.KFIN_APAGAR = P.KFIN_APAGAR'
      '              WHERE C.RECORRENCIA = '#39'T'#39
      '               AND P.VENCTO BETWEEN :DI AND :DF'
      '           ) PA ON PA.KFIN_APAGAR = A.KFIN_APAGAR'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_APAGAR'#39'   AND AP.TABLEKEY =  A.KFIN_APAGAR'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( A.TABLENAME IS NULL'
      '             OR (         A.TABLENAME = '#39'CMP_PEDIDOS'#39
      '                     AND A.SYS$ORIGIN_TYPE = 2 '
      
        '                     AND (( A.RECORRENCIA = '#39'F'#39' AND A.VALOR IS N' +
        'OT NULL ) OR ( A.RECORRENCIA = '#39'T'#39' AND PA.VALOR IS NOT NULL ))'
      
        '                     AND A.HISTORICO NOT CONTAINING '#39'Nota fiscal' +
        ' de compra'#39
      
        '                     AND A.HISTORICO NOT CONTAINING '#39'DUPLICATA D' +
        'E NOTA FISCAL'#39
      
        '                     AND A.HISTORICO NOT CONTAINING '#39'JUROS DE NO' +
        'TA FISCAL'#39
      '                ) '
      '          )'
      '  -- AND ( PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL )'
      
        '  AND ( IIF ( A.RECORRENCIA = '#39'F'#39', A.DATALANCTO, PA.VENCTO ) BET' +
        'WEEN :DI AND :DF )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
        'ULL ) '
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '   CAST( :PLANOCONTA_DESPESAS AS SYS$FKGUID20 )'
      ' , '#39'D'#39' DEBITO_CREDITO'
      ' , P.PAGTO'
      '-- , '#39'Juros '#39' || A.HISTORICO'
      
        ' , (COALESCE(  AP.PORCENTAGEM / 100.00, 1.00) * ABS( P.JURO_DESC' +
        'ONTO ) ) VALOR'
      'FROM FIN_APAGAR_PARCELAS P'
      'INNER JOIN FIN_APAGAR A ON A.KFIN_APAGAR = P.KFIN_APAGAR'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_APAGAR_PARCELAS'#39' AND AP.TABLEKEY =  P.KFIN_APAGA' +
        'R_PARCELA'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( P.KFIN_PLANOCONTA_JURO_DESCONTO = :PLANOCONTA_DESPESAS )'
      '  AND ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( P.PAGTO BETWEEN :DI AND :DF )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '   CAST( :PLANOCONTA_DESPESAS AS SYS$FKGUID20 )'
      ' , '#39'D'#39' DEBITO_CREDITO'
      ' , P.PAGTO'
      '-- , '#39'Desconto '#39' || R.HISTORICO'
      
        ' , COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * ABS( P.JURO_DESCON' +
        'TO )  VALOR'
      'FROM FIN_ARECEBER_PARCELAS P'
      'INNER JOIN FIN_ARECEBER R ON R.KFIN_ARECEBER = P.KFIN_ARECEBER'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_ARECEBER_PARCELAS'#39' AND AP.TABLEKEY =  P.KFIN_ARE' +
        'CEBER_PARCELA'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE ( P.KFIN_PLANOCONTA_JURO_DESCONTO = :PLANOCONTA_DESPESAS )'
      '  AND ( R.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( R.ATIVO = '#39'T'#39' )'
      '  AND ( P.PAGTO BETWEEN :DI AND :DF )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '   CMPI.KFIN_PLANOCONTA'
      ' , '#39'D'#39' DEBITO_CREDITO'
      ' , CMP.DATA_COMPRA'
      '-- , COALESCE ( P.NOME, CMPI.NOME )'
      
        ' , (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * CMPI.TOTAL  ) VAL' +
        'OR'
      'FROM CMP_PEDIDOS CMP'
      
        'LEFT JOIN CMP_PEDIDO_ITENS CMPI ON CMPI.KCMP_PEDIDO = CMP.KCMP_P' +
        'EDIDO'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'CMP_PEDIDO_ITENS'#39'   AND AP.TABLEKEY =  CMPI.KCMP_PED' +
        'IDO_ITEM'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      
        'LEFT JOIN EST_PRODUTOS P ON P.KEST_PRODUTO = (SELECT * FROM EST_' +
        'GET_PRODUTO_JUNCTION( CMPI.KEST_PRODUTO ))'
      'WHERE ( CMP.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( CMP.ATIVO = '#39'T'#39' )'
      '  AND ( CMP.DATA_COMPRA BETWEEN :DI AND :DF )'
      '  AND ( CMPI.KCMP_PEDIDO_ITEM IS NOT NULL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '   CMPI.KFIN_PLANOCONTA'
      ' , '#39'D'#39' DEBITO_CREDITO'
      ' , CMP.DATA_COMPRA'
      '-- , '#39'Frete: '#39' || COALESCE ( P.NOME, CMPI.NOME )'
      
        ' , (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * CMPI.FRETE  ) VAL' +
        'OR'
      'FROM CMP_PEDIDOS CMP'
      
        'LEFT JOIN CMP_PEDIDO_ITENS CMPI ON CMPI.KCMP_PEDIDO = CMP.KCMP_P' +
        'EDIDO'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'CMP_PEDIDO_ITENS'#39'   AND AP.TABLEKEY =  CMPI.KCMP_PED' +
        'IDO_ITEM'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      
        'LEFT JOIN EST_PRODUTOS P ON P.KEST_PRODUTO = (SELECT * FROM EST_' +
        'GET_PRODUTO_JUNCTION( CMPI.KEST_PRODUTO ))'
      'WHERE ( CMP.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( CMP.ATIVO = '#39'T'#39' )'
      '  AND ( CMP.DATA_COMPRA BETWEEN :DI AND :DF )'
      '  AND ( CMPI.KCMP_PEDIDO_ITEM IS NOT NULL )'
      '  AND ( CMPI.FRETE > 0 )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '    CMPS.KFIN_PLANOCONTA'
      '  , '#39'D'#39' DEBITO_CREDITO'
      '  , CMP.DATA_COMPRA'
      '--  , COALESCE ( P.NOME, CMPS.NOME )'
      
        '  , (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * CMPS.TOTAL  ) VA' +
        'LOR'
      'FROM CMP_PEDIDOS CMP'
      
        'LEFT JOIN CMP_PEDIDO_SERVICOS CMPS ON CMPS.KCMP_PEDIDO = CMP.KCM' +
        'P_PEDIDO'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'CMP_PEDIDO_SERVICOS'#39'   AND AP.TABLEKEY = CMPS.KCMP_P' +
        'EDIDO_SERVICO'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      
        'LEFT JOIN EST_PRODUTOS P ON P.KEST_PRODUTO = (SELECT * FROM EST_' +
        'GET_PRODUTO_JUNCTION( CMPS.KEST_PRODUTO ))'
      'WHERE ( CMP.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( CMP.ATIVO = '#39'T'#39' )'
      '  AND ( CMP.DATA_COMPRA BETWEEN :DI AND :DF )'
      '  AND ( CMPS.KCMP_PEDIDO_SERVICO IS NOT NULL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '    A.KFIN_PLANOCONTA'
      '  , '#39'D'#39' DEBITO_CREDITO'
      '  , PA.PAGTO'
      '--  , A.HISTORICO -- CMPI.NOME'
      
        '  , (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * (PA.VALOR * (CMP' +
        'I.TOTAL / COALESCE(NULLIF(CMP.TOTAL_NOTA,0),1) ))  ) VALOR'
      'FROM CMP_PEDIDOS CMP'
      
        'LEFT JOIN FIN_APAGAR  A ON A.TABLENAME = '#39'CMP_PEDIDOS'#39' AND A.TAB' +
        'LEKEY =  CMP.KCMP_PEDIDO'
      
        'LEFT JOIN FIN_APAGAR_PARCELAS  PA ON PA.KFIN_APAGAR  = A.KFIN_AP' +
        'AGAR'
      'LEFT JOIN CMP_PEDIDO_ITENS CMPI USING ( KCMP_PEDIDO )'
      
        'LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  = A.KFIN_PL' +
        'ANOCONTA'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'CMP_PEDIDO_ITENS'#39'   AND AP.TABLEKEY =  CMPI.KCMP_PED' +
        'IDO_ITEM'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE ( CMP.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.KFIN_PLANOCONTA = :PLANOCONTA_DESPESAS )'
      '  AND ( CMP.ATIVO = '#39'T'#39' )'
      '  AND ( PA.PAGTO BETWEEN :DI AND :DF )'
      '  AND ( CMPI.KCMP_PEDIDO_ITEM IS NOT NULL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT '
      ' F.KFIN_PLANOCONTA'
      ', '#39'D'#39' DEBITO_CREDITO'
      ', P.PAGTO'
      '--, '#39'Juros '#39' || A.HISTORICO'
      
        ', (COALESCE(  AP.PORCENTAGEM / 100.00, 1.00) * ABS( P.JURO_DESCO' +
        'NTO ) ) VALOR'
      'FROM FIN_FINANCIAMENTOS F'
      
        'INNER JOIN FIN_APAGAR A ON  A.KFIN_APAGAR = F.KFIN_APAGAR_AMORTI' +
        'ZACAO'
      'LEFT JOIN FIN_APAGAR_PARCELAS P ON P.KFIN_APAGAR = A.KFIN_APAGAR'
      
        'LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  = F.KFIN_PL' +
        'ANOCONTA -- PLANOCONTA_JUROS_FINANCIAMENTO'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_FINANCIAMENTOS'#39' AND AP.TABLEKEY =  F.KFIN_FINANC' +
        'IAMENTO'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  --AND ( P.JURO_DESCONTO > 0 )'
      '  -- AND ( PROGRAMACAO = '#39'T'#39' OR P.PAGTO IS NOT NULL )'
      '  AND ( P.PAGTO BETWEEN :DI AND :DF )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR P.PAGTO IS NOT NU' +
        'LL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      '  AND ( PC.KFIN_PLANOCONTA = :PLANOCONTA_JUROS_FINANCIAMENTO )'
      'UNION ALL'
      'SELECT '
      '  PC.KFIN_PLANOCONTA'
      ', '#39'D'#39' DEBITO_CREDITO'
      ', P.PAGTO'
      '--, '#39'Amortiza'#231#227'o '#39' || A.HISTORICO'
      
        ', (COALESCE(  AP.PORCENTAGEM / 100.00, 1.00) * ABS( P.VALOR ) ) ' +
        'VALOR'
      'FROM FIN_FINANCIAMENTOS F'
      
        'INNER JOIN FIN_APAGAR A ON  A.KFIN_APAGAR = F.KFIN_APAGAR_AMORTI' +
        'ZACAO'
      'LEFT JOIN FIN_APAGAR_PARCELAS P ON P.KFIN_APAGAR = A.KFIN_APAGAR'
      
        'LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  = :PLANOCON' +
        'TA_FINANCIAMENTO --F.KFIN_PLANOCONTA'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_FINANCIAMENTOS'#39' AND AP.TABLEKEY =  F.KFIN_FINANC' +
        'IAMENTO'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  -- AND ( PROGRAMACAO = '#39'T'#39' OR P.PAGTO IS NOT NULL )'
      '  AND ( P.PAGTO BETWEEN :DI AND :DF )'
      '  AND ( P.VALOR > 0 )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR P.PAGTO IS NOT NU' +
        'LL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT '
      '  PC.KFIN_PLANOCONTA'
      ', '#39'D'#39' DEBITO_CREDITO'
      ', P.PAGTO'
      '--, '#39'Despesas '#39' || A.HISTORICO'
      
        ', (COALESCE(  AP.PORCENTAGEM / 100.00, 1.00) * ABS( P.VALOR ) ) ' +
        'VALOR'
      'FROM FIN_FINANCIAMENTOS F'
      
        'INNER JOIN FIN_APAGAR A ON A.KFIN_APAGAR = F.KFIN_APAGAR_DESPESA' +
        'S'
      
        'LEFT JOIN  FIN_APAGAR_PARCELAS P ON P.KFIN_APAGAR = A.KFIN_APAGA' +
        'R'
      
        'LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  = :PLANOCON' +
        'TA_DESPESAS --F.KFIN_PLANOCONTA'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_FINANCIAMENTOS'#39' AND AP.TABLEKEY =  F.KFIN_FINANC' +
        'IAMENTO'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  -- AND ( PROGRAMACAO = '#39'T'#39' OR P.PAGTO IS NOT NULL )'
      '  AND ( P.PAGTO BETWEEN :DI AND :DF )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR P.PAGTO IS NOT NU' +
        'LL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT '
      '   CAST( :PLANOCONTA_JUROS_FINANCIAMENTO AS SYS$FKGUID20 )'
      ' , '#39'D'#39' DEBITO_CREDITO'
      ' , P.VENCTO'
      '-- , A.HISTORICO || '#39' (Juros)'#39' HISTORICO'
      
        ', (COALESCE(  AP.PORCENTAGEM / 100.00, 1.00) * ABS( FP.JUROS ) )' +
        ' VALOR'
      'FROM FIN_APAGAR_PARCELAS P'
      'INNER JOIN FIN_APAGAR A ON A.KFIN_APAGAR = P.KFIN_APAGAR'
      
        'INNER JOIN FIN_FINANCIAMENTOS_PARCELAS FP ON P.KFIN_APAGAR_PARCE' +
        'LA = FP.KFIN_APAGAR_PARCELA'
      
        'LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  =  :PLANOCO' +
        'NTA_JUROS_FINANCIAMENTO'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_APAGAR'#39' AND AP.TABLEKEY =  A.KFIN_APAGAR'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'LEFT JOIN CAD_ENTIDADES C ON C.KCAD_ENTIDADE = A.KCAD_ENTIDADE'
      'WHERE'
      '      ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( FP.JUROS > 0 )'
      '  -- AND ( PROGRAMACAO = '#39'T'#39' OR P.PAGTO IS NOT NULL )'
      '  AND ( P.VENCTO BETWEEN :DI AND :DF )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR P.PAGTO IS NOT NU' +
        'LL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT '
      '  CAST ( :PLANOCONTA_FINANCIAMENTO AS SYS$FKGUID20 )'
      ', '#39'D'#39' DEBITO_CREDITO'
      ', P.VENCTO'
      '--, '#39'Amortiza'#231#227'o '#39' || A.HISTORICO HISTORICO'
      ', ABS( FP.AMORTIZACAO ) VALOR'
      'FROM FIN_APAGAR_PARCELAS P'
      
        'INNER JOIN FIN_FINANCIAMENTOS_PARCELAS FP ON P.KFIN_APAGAR_PARCE' +
        'LA = FP.KFIN_APAGAR_PARCELA'
      'INNER JOIN FIN_APAGAR A ON A.KFIN_APAGAR = P.KFIN_APAGAR'
      
        'LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  =  :PLANOCO' +
        'NTA_FINANCIAMENTO'
      'LEFT JOIN CAD_ENTIDADES C ON C.KCAD_ENTIDADE = A.KCAD_ENTIDADE'
      'WHERE'
      '      ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( FP.AMORTIZACAO > 0 )'
      '  -- AND ( PROGRAMACAO = '#39'T'#39' OR P.PAGTO IS NOT NULL )'
      '  AND ( P.VENCTO BETWEEN :DI AND :DF )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR P.PAGTO IS NOT NU' +
        'LL )'
      '  AND ( (  :TIPOCC = 0 )   OR  (  :TIPOCC = 1 ) )'
      'UNION ALL'
      'SELECT'
      '   DVPI.KFIN_PLANOCONTA'
      ' , '#39'D'#39' DEBITO_CREDITO'
      ' , DVP.DATA_COMPRA'
      '-- , COALESCE ( P.NOME, DVPI.NOME )'
      
        ' , IIF( (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * DVPI.TOTAL  ' +
        ') > 0,'
      
        '     - (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * DVPI.TOTAL  )' +
        ','
      
        '       (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * DVPI.TOTAL  )' +
        ') VALOR'
      'FROM DEV_PEDIDOS DVP'
      
        'LEFT JOIN DEV_PEDIDO_ITENS DVPI ON DVPI.KDEV_PEDIDO = DVP.KDEV_P' +
        'EDIDO'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'DEV_PEDIDO_ITENS'#39'   AND AP.TABLEKEY =  DVPI.KDEV_PED' +
        'IDO_ITEM'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      
        'LEFT JOIN EST_PRODUTOS P ON P.KEST_PRODUTO = (SELECT * FROM EST_' +
        'GET_PRODUTO_JUNCTION( DVPI.KEST_PRODUTO ))'
      'WHERE ( DVP.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( DVP.ATIVO = '#39'T'#39' )'
      '  AND ( DVP.DATA_COMPRA BETWEEN :DI AND :DF )'
      '  AND ( DVPI.KDEV_PEDIDO_ITEM IS NOT NULL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '   CAST ( :PLANOCONTA_IMPOSTOS_TAXAS AS SYS$FKGUID20 )'
      ' , '#39'D'#39' DEBITO_CREDITO'
      ' , VND.DATA_VENDA'
      '-- , '#39'Dedu'#195#167#195#181'es: NF '#39' || VND.NOTA_FISCAL'
      
        ', (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * (VND.VALOR_DEDUCOE' +
        'S * (VNDI.TOTAL / COALESCE(NULLIF(VND.TOTAL_NOTA,0),1) ))  ) VAL' +
        'OR'
      'FROM VND_PEDIDOS VND'
      
        'LEFT JOIN VND_PEDIDO_ITENS VNDI ON VNDI.KVND_PEDIDO = VND.KVND_P' +
        'EDIDO'
      
        'INNER JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  = :PLANOCO' +
        'NTA_IMPOSTOS_TAXAS'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'VND_PEDIDO_ITENS'#39'   AND AP.TABLEKEY = VNDI.KVND_PEDI' +
        'DO_ITEM'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( VND.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( VND.ATIVO = '#39'T'#39' )'
      '  AND ( VND.DATA_VENDA BETWEEN :DI AND :DF )'
      '  AND ( VNDI.KVND_PEDIDO_ITEM IS NOT NULL )'
      '  AND ( VND.VALOR_DEDUCOES > 0 )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT -- receitas'
      '   A.KFIN_PLANOCONTA'
      ' , '#39'C'#39' DEBITO_CREDITO'
      
        ' , IIF ( A.RECORRENCIA = '#39'F'#39', A.DATALANCTO, PA.VENCTO ) DATALANC' +
        'TO'
      '-- , A.HISTORICO'
      
        ' , (COALESCE( AP.PORCENTAGEM / 100.00, 1.00) * IIF ( A.RECORRENC' +
        'IA = '#39'F'#39', A.VALOR, PA.VALOR ) ) VALOR'
      'FROM FIN_ARECEBER A'
      
        'LEFT JOIN ( SELECT P.KFIN_ARECEBER, P.VENCTO, P.PRORROGACAO, P.P' +
        'AGTO, P.VALOR'
      '              FROM FIN_ARECEBER_PARCELAS P'
      
        '              JOIN FIN_ARECEBER C ON C.KFIN_ARECEBER = P.KFIN_AR' +
        'ECEBER'
      '             WHERE C.RECORRENCIA = '#39'T'#39
      '               AND P.VENCTO BETWEEN :DI AND :DF'
      '           ) PA ON PA.KFIN_ARECEBER = A.KFIN_ARECEBER'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'FIN_ARECEBER'#39' AND AP.TABLEKEY  = A.KFIN_ARECEBER'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( A.TABLENAME IS NULL )'
      '  -- AND ( PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL )'
      
        '  AND ( IIF ( A.RECORRENCIA = '#39'F'#39', A.DATALANCTO, PA.VENCTO ) BET' +
        'WEEN :DI AND :DF )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
        'ULL ) '
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '    A.KFIN_PLANOCONTA'
      '  , '#39'C'#39' DEBITO_CREDITO'
      
        '  , IIF ( A.RECORRENCIA = '#39'F'#39', A.DATALANCTO, PA.VENCTO ) DATALAN' +
        'CTO'
      '--  , A.HISTORICO'
      '  , PA.VALOR VALOR'
      'FROM FIN_ARECEBER A'
      
        'LEFT JOIN ( SELECT P.KFIN_ARECEBER, P.VENCTO, P.PRORROGACAO, P.P' +
        'AGTO, P.VALOR'
      '              FROM FIN_ARECEBER_PARCELAS P'
      
        '              JOIN FIN_ARECEBER C ON C.KFIN_ARECEBER = P.KFIN_AR' +
        'ECEBER'
      '             WHERE P.VENCTO BETWEEN :DI AND :DF'
      '           ) PA ON PA.KFIN_ARECEBER = A.KFIN_ARECEBER'
      'WHERE ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( A.TABLENAME = '#39'FIN_FINANCIAMENTOS_GERAL'#39' )'
      '  -- AND ( PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL )'
      
        '  AND ( IIF( A.RECORRENCIA = '#39'F'#39', A.DATALANCTO, PA.VENCTO ) BETW' +
        'EEN :DI AND :DF )'
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
        'ULL )'
      '  AND  (  :TIPOCC = 0  OR  :TIPOCC = 1 )'
      'UNION ALL'
      'SELECT'
