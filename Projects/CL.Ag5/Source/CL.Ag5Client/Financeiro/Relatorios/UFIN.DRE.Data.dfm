      'SELECT'
      '  VNDS.KFIN_PLANOCONTA'
      ', CAST( NULL AS SYS$FKGUID20 ) KEST_GRUPOPRODUTO'
      ', '#39'C'#39' DEBITO_CREDITO'
      ', COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) DATA'
      
        ', (COALESCE( AP.PORCENTAGEM / 100.00, 1.00 ) * (PA.VALOR * (VNDS' +
        '.TOTAL / COALESCE(NULLIF(VND.TOTAL_NOTA,0),1) ))  ) VALOR'
      'FROM VND_PEDIDOS VND'
      
        'LEFT JOIN VND_PEDIDO_SERVICOS VNDS ON VNDS.KVND_PEDIDO = VND.KVN' +
        'D_PEDIDO'
      
        'LEFT JOIN FIN_ARECEBER           A ON A.TABLENAME = '#39'VND_PEDIDOS' +
        #39' AND A.TABLEKEY = VND.KVND_PEDIDO'
      
        'LEFT JOIN FIN_ARECEBER_PARCELAS  PA ON PA.KFIN_ARECEBER = A.KFIN' +
        '_ARECEBER'
      
        '-- LEFT JOIN FIN_PLANOCONTAS  PC ON PC.KFIN_PLANOCONTA  = VNDS.K' +
        'FIN_PLANOCONTA'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'VND_PEDIDO_SERVICOS'#39' AND AP.TABLEKEY  = VNDS.KVND_PE' +
        'DIDO_SERVICO'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( VND.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( VND.ATIVO = '#39'T'#39' )'
      '  AND ( :PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL ) '
      
        '  AND ( COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) BETWEEN' +
        ' :DI AND :DF ) '
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
        'ULL ) '
      '  AND ( VNDS.KVND_PEDIDO_SERVICO IS NOT NULL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '  '#39'financiamentos'#39
      ', CAST( NULL AS SYS$FKGUID20 ) KEST_GRUPOPRODUTO'
      ', '#39'C'#39' DEBITO_CREDITO'
      ', COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) DATA'
      ', PA.VALOR VALOR'
      'FROM FIN_ARECEBER A'
      
        'LEFT JOIN FIN_ARECEBER_PARCELAS PA ON PA.KFIN_ARECEBER = A.KFIN_' +
        'ARECEBER'
      'WHERE'
      '      ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( A.TABLENAME = '#39'FIN_FINANCIAMENTOS_GERAL'#39' )'
      '  AND ( :PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL )'
      
        '  AND ( COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) BETWEEN' +
        ' :DI AND :DF ) '
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
        'ULL ) '
      '  AND (  :TIPOCC = 0  OR  :TIPOCC = 1 )'
      'UNION ALL'
      'SELECT'
      '  '#39'financiamentos'#39
      ', CAST( NULL AS SYS$FKGUID20 ) KEST_GRUPOPRODUTO'
      ', '#39'C'#39' DEBITO_CREDITO'
      ', COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) DATA'
      ', PA.VALOR VALOR'
      'FROM FIN_FINANCIAMENTOS F'
      
        'LEFT JOIN FIN_ARECEBER A ON A.KFIN_ARECEBER  = F.KFIN_ARECEBER_F' +
        'INANCIAMENTO '
      
        'LEFT JOIN FIN_ARECEBER_PARCELAS PA ON PA.KFIN_ARECEBER = A.KFIN_' +
        'ARECEBER'
      'WHERE'
      '      ( A.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( A.ATIVO = '#39'T'#39' )'
      '  AND ( :PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL )'
      
        '  AND ( COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) BETWEEN' +
        ' :DI AND :DF ) '
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
        'ULL ) '
      '  AND (  :TIPOCC = 0  OR  :TIPOCC = 1 )'
      'UNION ALL'
      'SELECT'
      '  '#39'litros'#39
      ', CAST( NULL AS SYS$FKGUID20 ) KEST_GRUPOPRODUTO'
      ', '#39'C'#39' DEBITO_CREDITO'
      ', COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) DATA'
      ',    VNDI.QTDE'
      
        '   / (VNDI.TOTAL*(1-VND.VALOR_DEDUCOES/COALESCE(NULLIF(VND.TOTAL' +
        '_NOTA,0),1.00)))'
      '   * COALESCE( AP.PORCENTAGEM / 100.00, 1.00 )'
      '   *PA.VALOR'
      '   *VNDI.TOTAL'
      
        '   *(1-VND.VALOR_DEDUCOES/COALESCE(NULLIF(VND.TOTAL_NOTA,0),1.00' +
        '))'
      '   / COALESCE(NULLIF(VND.TOTAL_NOTA-VND.VALOR_DEDUCOES,0),1.00)'
      '  VALOR'
      'FROM VND_PEDIDOS VND'
      
        'LEFT JOIN VND_PEDIDO_ITENS VNDI ON VNDI.KVND_PEDIDO = VND.KVND_P' +
        'EDIDO'
      
        'LEFT JOIN FIN_ARECEBER           A ON A.TABLENAME = '#39'VND_PEDIDOS' +
        #39' AND A.TABLEKEY = VND.KVND_PEDIDO'
      
        'LEFT JOIN FIN_ARECEBER_PARCELAS  PA ON PA.KFIN_ARECEBER = A.KFIN' +
        '_ARECEBER'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'VND_PEDIDO_ITENS'#39' AND AP.TABLEKEY  = VNDI.KVND_PEDID' +
        'O_ITEM'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( VND.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( VNDI.KFIN_PLANOCONTA = :KFIN_PLANOCONTA_LEITE )'
      '  AND ( VND.ATIVO = '#39'T'#39' )'
      '  AND ( :PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL ) '
      
        '  AND ( COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) BETWEEN' +
        ' :DI AND :DF ) '
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
        'ULL ) '
      '  AND ( VNDI.KVND_PEDIDO_ITEM IS NOT NULL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'UNION ALL'
      'SELECT'
      '  '#39'valor_medio'#39' KFIN_PLANOCONTA'
      ', CAST( NULL AS SYS$FKGUID20 ) KEST_GRUPOPRODUTO'
      ', '#39'C'#39' DEBITO_CREDITO'
      ',   COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO )'
      
        '  - EXTRACT( DAY FROM  COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.V' +
        'ENCTO ) )'
      '  + 1 DATA'
      ',   SUM (   COALESCE(AP.PORCENTAGEM/100.00,1.00)'
      '          * PA.VALOR'
      '          * VNDI.TOTAL'
      
        '          * (1 - VND.VALOR_DEDUCOES/COALESCE(NULLIF(VND.TOTAL_NO' +
        'TA,0),1))'
      
        '          / COALESCE(NULLIF( VND.TOTAL_NOTA-VND.VALOR_DEDUCOES,0' +
        ' ), 1)'
      '        ) /* valor eqv parcela*/'
      '  / SUM ( VNDI.QTDE'
      
        '         / (VNDI.TOTAL*(1-VND.VALOR_DEDUCOES/COALESCE(NULLIF(VND' +
        '.TOTAL_NOTA,0),1.00)))'
      '         * COALESCE( AP.PORCENTAGEM / 100.00, 1.00 )'
      '         * PA.VALOR'
      '         * VNDI.TOTAL'
      
        '         * (1-VND.VALOR_DEDUCOES/COALESCE(NULLIF(VND.TOTAL_NOTA,' +
        '0),1.00))'
      
        '         / COALESCE(NULLIF(VND.TOTAL_NOTA-VND.VALOR_DEDUCOES,0),' +
        '1.00)'
      '        ) VALOR'
      'FROM VND_PEDIDOS VND'
      
        'LEFT JOIN VND_PEDIDO_ITENS VNDI ON VNDI.KVND_PEDIDO = VND.KVND_P' +
        'EDIDO'
      
        'LEFT JOIN FIN_ARECEBER           A ON A.TABLENAME = '#39'VND_PEDIDOS' +
        #39' AND A.TABLEKEY = VND.KVND_PEDIDO'
      
        'LEFT JOIN FIN_ARECEBER_PARCELAS  PA ON PA.KFIN_ARECEBER = A.KFIN' +
        '_ARECEBER'
      
        'LEFT JOIN FIN_APROPRIACAO  AP ON :USE_APROPRIACAO = '#39'T'#39' AND AP.T' +
        'ABLENAME = '#39'VND_PEDIDO_ITENS'#39' AND AP.TABLEKEY  = VNDI.KVND_PEDID' +
        'O_ITEM'
      
        'LEFT JOIN FIN_CENTROSCUSTO CC ON CC.KFIN_CENTROCUSTO = AP.KFIN_C' +
        'ENTROCUSTO'
      'WHERE'
      '      ( VND.KSYS$DOMAIN = :KSYS$DOMAIN )'
      '  AND ( VNDI.KFIN_PLANOCONTA = :KFIN_PLANOCONTA_LEITE )'
      '  AND ( VND.ATIVO = '#39'T'#39' )'
      '  AND ( :PROGRAMACAO = '#39'T'#39' OR PA.PAGTO IS NOT NULL ) '
      
        '  AND ( COALESCE ( PA.PAGTO, PA.PRORROGACAO, PA.VENCTO ) BETWEEN' +
        ' :DI AND :DF ) '
      
        '  AND ( :PROVISAO = '#39'T'#39' OR A.PROVISAO = '#39'F'#39' OR PA.PAGTO IS NOT N' +
        'ULL ) '
      '  AND ( VNDI.KVND_PEDIDO_ITEM IS NOT NULL )'
      '  AND (        (  :TIPOCC = 0 )'
      
        '            OR ( (  :TIPOCC = 1 ) AND (  CC.KFIN_CENTROCUSTO IS ' +
        'NULL ) )'
      
        '            OR ( (  :TIPOCC = 2 ) AND (  CC.KFIN_CENTROCUSTO  = ' +
        ':CC OR CC.CLASSIFICACAO LIKE :CLASSIFICACAOCC ) )'
      '         )'
      'GROUP BY 1,2,3,4'
      ')'
      'GROUP BY 1,2,3,4'
      'ORDER BY 1,2,3')
